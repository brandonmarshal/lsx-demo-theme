    "content": "<!-- wp:group {\"className\":\"fish-details-section\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"var:preset|spacing|60\",\"bottom\":\"var:preset|spacing|60\"},\"blockGap\":\"var:preset|spacing|50\"}},\"layout\":{\"type\":\"constrained\"}} -->\n<div class=\"wp-block-group fish-details-section\" style=\"padding-top:var(--wp--preset--spacing--60);padding-bottom:var(--wp--preset--spacing--60)\"><!-- wp:columns {\"verticalAlignment\":\"top\"} -->\n<div class=\"wp-block-columns are-vertically-aligned-top\"><!-- wp:column {\"verticalAlignment\":\"top\",\"width\":\"66.66%\"} -->\n<div class=\"wp-block-column is-vertically-aligned-top\" style=\"flex-basis:66.66%\"><!-- wp:heading -->\n<h2 class=\"wp-block-heading\">Species Information</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p>Learn about this fish species including habitat, behavior, and fishing techniques.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:fishing-cpt-plugin/repeatable-facts /-->\n\n<!-- wp:separator {\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator has-alpha-channel-opacity is-style-wide\"/>\n<!-- /wp:separator -->\n\n<!-- wp:heading {\"level\":3} -->\n<h3 class=\"wp-block-heading\">Description</h3>\n<!-- /wp:heading -->\n\n<!-- wp:post-content {\"layout\":{\"type\":\"constrained\"}} /--></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"verticalAlignment\":\"top\",\"width\":\"33.33%\"} -->\n<div class=\"wp-block-column is-vertically-aligned-top\" style=\"flex-basis:33.33%\"><!-- wp:group {\"className\":\"fish-quick-facts-box\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"var:preset|spacing|50\",\"right\":\"var:preset|spacing|50\",\"bottom\":\"var:preset|spacing|50\",\"left\":\"var:preset|spacing|50\"}},\"border\":{\"width\":\"1px\"}},\"borderColor\":\"accent-6\",\"backgroundColor\":\"base-2\",\"layout\":{\"type\":\"constrained\"}} -->\n<div class=\"wp-block-group fish-quick-facts-box has-border-color has-accent-6-border-color has-base-2-background-color has-background\" style=\"border-width:1px;padding-top:var(--wp--preset--spacing--50);padding-right:var(--wp--preset--spacing--50);padding-bottom:var(--wp--preset--spacing--50);padding-left:var(--wp--preset--spacing--50)\"><!-- wp:heading {\"level\":3} -->\n<h3 class=\"wp-block-heading\">Quick Facts</h3>\n<!-- /wp:heading -->\n\n<!-- wp:fishing-cpt-plugin/fish-facts /-->\n\n<!-- wp:separator {\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator has-alpha-channel-opacity is-style-wide\"/>\n<!-- /wp:separator -->\n\n<!-- wp:group {\"className\":\"fish-metadata\",\"style\":{\"spacing\":{\"blockGap\":\"var:preset|spacing|30\"}},\"layout\":{\"type\":\"flex\",\"orientation\":\"vertical\"}} -->\n<div class=\"wp-block-group fish-metadata\"><!-- wp:paragraph {\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\"><strong>Water Type:</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\",\"metadata\":{\"bindings\":{\"content\":{\"source\":\"fishing-cpt-plugin/scf-fields\",\"args\":{\"field_name\":\"water_type\"}}}}} -->\n<p class=\"has-small-font-size\">Freshwater</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\"><strong>Average Size:</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\",\"metadata\":{\"bindings\":{\"content\":{\"source\":\"fishing-cpt-plugin/scf-fields\",\"args\":{\"field_name\":\"average_size\"}}}}} -->\n<p class=\"has-small-font-size\">12-18 inches</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\"><strong>Bait Type:</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\",\"metadata\":{\"bindings\":{\"content\":{\"source\":\"fishing-cpt-plugin/scf-fields\",\"args\":{\"field_name\":\"bait_type\"}}}}} -->\n<p class=\"has-small-font-size\">Lures, Live Bait</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:group --></div>\n<!-- /wp:group --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns --></div>\n<!-- /wp:group -->"

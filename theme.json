                },
                {
                    "name": "Neutral 400",
                    "slug": "neutral-400",
                    "color": "#B3B7B0"
                },
                {
                    "name": "Neutral 500",
                    "slug": "neutral-500",
                    "color": "#8C9188"
                },
                {
                    "name": "Neutral 600",
                    "slug": "neutral-600",
                    "color": "#6A6F66"
